{
  "manifest_version": 2,

  "name": "UPA - The Url Parser",
  "description": "Url parser that works with #. Useful for SPA. Visualize and basic editing.",
  "version": "1.2",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "activeTab"]
}
